var fluffpost_images = [];
  
var withFloof = function(callback) {
  return function(elem) {
    if(fluffpost_images.length == 0) {
      console.log('refilling floof pool');
      fluffpost_images = shuffle([].concat(images));
    }
    callback(elem, fluffpost_images.shift());
  }
}

var easter_egg = new Konami(function() {
  document.querySelectorAll('div.card__image__src, .card__author-headshot__src').forEach(withFloof(function(div, floof){
    var old_image = div.style.backgroundImage;
	  div.style.backgroundImage = old_image.replace(/(url\("https:\/\/img\.huffingtonpost\.com\/asset\/)[\d\w]+(\..*"\))/, "$1"+floof+"$2");
  }))
  document.querySelectorAll('img.card__image__src').forEach(withFloof(function(img, floof){
    if(!!img.src.match(/^data:/)) {
      img.src = 'https://img.huffingtonpost.com/asset/'+floof+'.jpeg?ops='+img.width+'_'+img.height+',quality_75'
    } else {
	    img.src = img.src.replace(/(https:\/\/img\.huffingtonpost\.com\/asset\/)[\d\w]+(\..*)/, "$1"+floof+"$2");
    }
  }));
  document.querySelectorAll('.plr-card__image__wrapper, .o2-cvs-video-thumbnail').forEach(withFloof(function(div, floof){
    div.style.backgroundImage = 'url(https://img.huffingtonpost.com/asset/'+floof+'.jpeg?ops='+(div.offsetWidth*2)+'_'+(div.offsetHeight*2)+',quality_75)'
  }));
  document.querySelector('.logo__img').src = "https://big.assets.huffingtonpost.com/athena/files/2019/01/02/5c2d03abe4b05c88b7052828.svg";
});
}());
